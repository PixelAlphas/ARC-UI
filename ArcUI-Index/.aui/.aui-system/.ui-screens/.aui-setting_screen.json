{ "namespace": "aui-setting_screen",
  "root_contents@auicore_configloader.core": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "$auivar:back_button": true,
    "controls": [
      { "status_bottom_bar@aui-common.status_bottom_bar": { "$auivar-screen_name": "\"Settings Screen\"", "$auivar-label_no_mcver": true }},
      { "menu_title@aui-common.menu_title": { "$auivar-title_text": "SETTINGS" }},
      { "stack_panel": {
          "type": "stack_panel",
          "size": [ "100%", "100% - 34px" ],
          "controls": [
            { "content_panel": {
                "type": "panel",
                "size": [ "100%", "fill" ],
                "controls": [
                  { "container": {
                      "type": "panel",
                      "size": [ "100%-16px","100%" ],
                      "controls": [
                        { "$dialog_content@$dialog_content": {} }
                  ]}}
            ]}}
      ]}},
      { "popup_dialog_factory": {
          "type": "factory",
          "control_ids": {
            "purchase_failed": "@realms_create.popup_dialog__purchase_failed",
            "purchase_in_progress": "@realms_create.popup_dialog__purchase_in_progress",
            "share_by_link": "@realms_allowlist.popup_dialog__share_by_link",
            "options_modal": "@realms_allowlist.popup_dialog__options",
            "backups_modal": "@realms_settings.popup_dialog__backups"
      }}}
  ]},
  "dialog_content": {
    "type": "panel",
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "controls": [
      { "selector_area@settings_common.selector_area": {
          "$auivar:anim_from|default": [ "-25%", 16 ],
          "$auivar:anim_to|default": [ 0, 16 ],
          "offset": [ 2, 16 ],
          "animation_reset_name": "screen_animation_reset",
          "anims": [
            "@aui-anim.screen_outro_anim_push",
            "@aui-anim.screen_outro_anim_pop",
            "@aui-anim.screen_intro_anim_push",
            "@aui-anim.screen_intro_anim_pop"
      ]}},
      { "content_area@settings_common.content_area": {
          "$auivar:anim_from|default": [ "25%", 16 ],
          "$auivar:anim_to|default": [ 0, 16 ],
          "offset": [ 0, 16 ],
          "animation_reset_name": "screen_animation_reset",
          "anims": [
            "@aui-anim.screen_outro_anim_push",
            "@aui-anim.screen_outro_anim_pop",
            "@aui-anim.screen_intro_anim_push",
            "@aui-anim.screen_intro_anim_pop"
      ]}}
  ]},
  "selector_stack_panel": {
    "type": "stack_panel",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "orientation": "vertical",
    "$default_selector_toggle_index": 0,
    "ttsSectionContainer": true,
    "controls": [
      { "buttons_panel": {
          "type": "stack_panel",
          "controls": [
            { "pudding00": { "type": "panel", "size": [ 2, 2 ] }},
            { "header@aui-common.header": { "ignored": "(not $include_controls_and_settings_sections)", "$text": "Utilities" }},
            { "pudding01": { "type": "panel", "size": [ 2, 2 ] }},
            { "gamertag_core@aui-setting_screen.gamertag_core": { "ignored": "(not $include_controls_and_settings_sections)" }},
            { "pudding02": { "type": "panel", "size": [ 1, 1 ] }},
            { "action_button@aui-setting_screen.action_button": {
                "$pressed_button_name": "sign_out_button",
                "ignored": "(($build_platform_UWP or not $is_pregame or $requires_xbl_signin_to_play or $include_world_section)",
                "$button_text": "xbox.signOut",
                "size": [ "100%", 30 ]
            }},
            { "pudding03": { "type": "panel", "size": [ 1, 1 ], "ignored": "($build_platform_UWP or not $is_pregame or $requires_xbl_signin_to_play)" }},
            { "button_background": {
                "type": "image",
                "color": "$primary_color",
                "texture": ".aui/.aui-assets/buttons/global/black_button",
                "size": [ "100%", "100%cm + 4px" ],
                "controls": [
                  { "stack_panel": {
                      "type": "stack_panel",
                      "size": [ "100%", "100%c" ],
                      "controls": [
                        { "general_button@aui-setting_screen.general_button": { "$toggle_group_forced_index": "$general_forced_index", "ignored": "($device_must_be_removed_for_xbl_signin or $fire_tv or not $is_pregame or $disable_gamertag_controls or $include_world_section)" }},
                        { "accessibility_button@aui-setting_screen.accessibility_button": { "$toggle_group_forced_index": "$accessibility_forced_index", "ignored": "(not $include_controls_and_settings_sections)" }},
                        { "creator_button@aui-setting_screen.creator_button": { "$toggle_group_forced_index": 100, "ignored": "(not $include_controls_and_settings_sections)" }},
                        { "realm_selector_panel@realms_settings.selector_panel": { "ignored": "(not $include_realm_section)", "$world_toggle_index": "$world_forced_index", "$members_toggle_index": "$members_forced_index", "$subscription_toggle_index": "$subscription_forced_index", "$backup_toggle_index": "$backup_forced_index", "$dev_options_toggle_index": "$dev_options_forced_index"}}
                  ]}}
            ]}},
            { "world_selector_pane@aui-setting_screen.selector_pane_content": { "ignored": "(not $include_world_section)" }}
      ]}},
      { "controls_and_settings_selector_pane": {
          "type": "stack_panel",
          "ignored": "(not $include_controls_and_settings_sections)",
          "$controls_section_header": "options.group.input",
          "$general_section_header": "stat.generalButton",
          "controls": [
            { "pudding00": { "type": "panel", "size": [ 4, 4 ] }},
            { "header00@aui-common.header": { "$text": "Controls" }},
            { "pudding01": { "type": "panel", "size": [ 2, 2 ] }},
            { "button_background": {
                "type": "image",
                "color": "$primary_color",
                "texture": ".aui/.aui-assets/buttons/global/black_button",
                "size": [ "100%", "100%cm + 4px" ],
                "controls": [
                  { "stack_panel": {
                      "type": "stack_panel",
                      "ignored": "(not $include_controls_and_settings_sections)",
                      "size": [ "100%", "100%c" ],
                      "controls": [
                        { "how_to_play_button@aui-setting_screen.how_to_play_button": { "$toggle_group_forced_index": "$how_to_play_index", "ignored": "(not $include_controls_and_settings_sections)" }},
                        { "keyboard_and_mouse_button@aui-setting_screen.keyboard_and_mouse_button": { "ignored": "(not $keyboard_and_mouse_supported)", "$toggle_group_forced_index": "$keyboard_and_mouse_forced_index", "$tts_section_header": "$controls_section_header" }},
                        { "controller_button@aui-setting_screen.controller_button": { "ignored": "(not $gamepad_supported or $nx_os)", "$toggle_group_forced_index": "$controller_and_switch_forced_index", "$tts_section_header": "$controls_section_header" }},
                        { "switch_controller_button@aui-setting_screen.controller_button": { "ignored": "(not $gamepad_supported or not $nx_os)", "$toggle_group_forced_index": "$controller_and_switch_forced_index", "$glyph_texture": ".aui/.aui-assets/icons/glyph/controller_glyph_switch", "$glyph_color_texture": ".aui/.aui-assets/icons/glyph/controller_glyph_color_switch", "$tts_section_header": "$controls_section_header" }},
                        { "touch_button@aui-setting_screen.touch_button": { "ignored": "($is_holographic or not $touch_supported or $nx_os)", "$toggle_group_forced_index": "$touch_forced_index", "$tts_section_header": "$controls_section_header" }}
                  ]}}
            ]}},
            { "pudding02": { "type": "panel", "size": [ 4, 4 ] }},
            { "header01@aui-common.header": { "$text": "Generals" }},
            { "pudding03": { "type": "panel", "size": [ 2, 2 ] }},
            { "button_background": {
                "type": "image",
                "color": "$primary_color",
                "texture": ".aui/.aui-assets/buttons/global/black_button",
                "size": [ "100%", "100%cm + 4px" ],
                "controls": [
                  { "stack_panel": {
                      "type": "stack_panel",
                      "size": [ "100%", "100%c" ],
                      "controls": [
                        { "video_button@aui-setting_screen.video_button": { "$toggle_group_forced_index": "$video_forced_index", "$tts_section_header": "$general_section_header" }},
                        { "sound_button@aui-setting_screen.sound_button": { "$toggle_group_forced_index": "$sound_forced_index", "$tts_section_header": "$general_section_header" }},
                        { "global_texture_pack_button@aui-setting_screen.global_texture_pack_button": { "ignored": "$ignore_global_resources_section", "$toggle_group_forced_index": "$global_texture_pack_forced_index", "visible": "($is_global_texture_packs_visible)", "$tts_section_header": "$general_section_header" }},
                        { "language_button@aui-setting_screen.language_button": { "ignored": "(not $is_pregame)", "$toggle_group_forced_index": "$language_forced_index", "$tts_section_header": "$general_section_header" }}
                  ]}}
            ]}}
      ]}},
      { "addons_selector_panel@aui-setting_screen.addons_selector_panel": { "ignored": "(not $include_world_section and (not $include_realm_section or not $realms_pack_feature_enabled))", "$level_texture_pack_toggle_index": "$level_texture_pack_index", "$addon_toggle_index": "$addon_index" }}
  ]},
  "section_content_panels@auicore_configloader.core": {
    "type": "panel",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "size": [ "100% - 4px", "100%c + 4px" ],
    "offset": [ 2, 0 ],
    "controls": [
      { "world_sections": {
          "type": "stack_panel",
          "ignored": "(not $include_world_section)",
          "controls": [
            { "game_section@world_section.game_section": {} },
            { "server_section@world_section.server_section": {} },
            { "multiplayer_section@world_section.multiplayer_section": {} }
      ]}},
      { "packs_sections": {
          "type": "stack_panel",
          "ignored": "(not $include_world_section and (not $include_realm_section or not $realms_pack_feature_enabled))",
          "controls": [
            { "level_texture_pack_section@world_section.level_texture_pack_section": {} },
            { "addon_section@world_section.addon_section": {} }
      ]}},
      { "general_and_controls_sections": {
          "type": "stack_panel",
          "ignored": "(not $include_controls_and_settings_sections)",
          "controls": [
            { "accessibility_section@aui-setting_accessibility.root_section": { "visible": false } },
            { "keyboard_and_mouse_section@aui-km_control.root_section": { "ignored": "(not $keyboard_and_mouse_supported)", "visible": false }},
            { "controller_section@aui-gamepad_control.root_section": { "ignored": "(not $gamepad_supported)", "visible": false}},
            { "touch_section@aui-touch_control.root_section": { "ignored": "($is_holographic or not $touch_supported or $nx_os)", "visible": false}},
            { "general_tab_section@aui-setting_general.root_section": { "visible": false } },
            { "creator_section@aui-setting_creator.root_section": { "visible": false } },
            { "sound_section@aui-setting_sound.root_section": { "visible": false } },
            { "video_section@aui-setting_video.root_section": { "visible": false } },
            { "how_to_play_section@aui-setting_htp.root_section": { "visible": false } },
            { "language_section@aui-setting_lang.root_section": { "ignored": "(not $is_pregame)", "visible": false }},
            { "global_texture_pack_section@general_section.global_texture_pack_section": { "visible": false, "ignored": "$ignore_global_resources_section" }}
      ]}}
  ]},
  "selector_pane_content@auicore_configloader.core": {
    "type": "stack_panel",
    "$tts_section_header": "realmsSettingsScreen.worldSectionTitle",
    "variables": { "requires": "$is_pregame", "$tts_section_header": "options.editSettings" },
    "controls": [
      { "world_snapshot_image": {
          "ignored": "(not $is_pregame)",
          "type": "panel",
          "size": [ "100%", "100%c + 2px" ],
          "controls": [
            { "thumbnail": {
                "type": "image",
                "force_texture_reload": true,
                "size": [ "100% - 2px", "56.25%x + 2.25px" ],
                "bilinear": true,
                "layer": 2,
                "anchor_from": "top_middle",
                "anchor_to": "top_middle",
                "offset": [ 0, 1 ],
                "fill": true,
                "controls": [
                  { "border": {
                      "type": "image",
                      "size": [ "100% + 2px", "100% + 2px" ],
                      "texture": "textures/ui/BannersTransparent",
                      "anchor_from": "center",
                      "anchor_to": "center",
                      "layer": 1
                }}],
                "bindings": [
                  { "binding_name": "#world_texture_name",
                    "binding_name_override": "#texture"
                  },
                  { "binding_name": "#world_texture_locationtype",
                    "binding_name_override": "#texture_file_system"
                }]
            }}
      ]}},
      { "play_or_create_panel@aui-setting_screen.play_or_create_stack_bedrock": { "ignored": "(not $is_pregame or $education_edition)", "$button_text_max_size": [ "100%", 20 ] }},
      { "upload_world_panel@aui-setting_screen.upload_world_stack_bedrock": { "ignored": "(not $is_pregame or $education_edition)", "$button_text_max_size": [ "100%", 20 ] }},
      { "play_or_host_panel@world_section.play_or_host_stack_edu": {}},
      { "server_settings_visibility_panel": {
          "type": "stack_panel",
          "bindings": [
            { "binding_name": "#server_settings_visible",
              "binding_name_override": "#visible"
          }],
          "controls": [
            { "selector_group_label_0@settings_common.selector_group_label": {
                "ignored": "($is_pregame)",
                "text": "options.category.server"
            }},
            { "server_settings_button@world_section.server_settings_button": {
                "ignored": "($is_pregame)",
                "$toggle_group_forced_index": "$server_forced_index"
            }},
            { "server_spacer": {
                "type": "panel",
                "size": [ 0, 2 ]
            }}
      ]}},
      { "header00@aui-common.header": { "$text": "World", "ignored": "(not $is_pregame)" }},
      { "pudding00": { "type": "panel", "size": [ 2, 2 ] }},
      { "game_button@aui-setting_screen.game_button": { "visible": "(not $education_edition_china)", "$toggle_group_forced_index": "$game_forced_index" }},
      { "multiplayer_button@aui-setting_screen.multiplayer_button": { "visible": "(not $education_edition)", "$toggle_group_forced_index": "$multiplayer_forced_index" }}
  ]},
  "section_header_panelste": {
    "type": "panel",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "size": [ "100%", "100%c" ]
  },
  "section_footer_panelste": {
    "type": "panel",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "size": [ "100%", "100%c" ]
  },
  "section_header_panels": {
    "type": "panel",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "size": [ "100%", "100%c" ],
    "offset": [ 2, 0 ],
    "controls": [
      { "world_sections": {
          "type": "stack_panel",
          "ignored": "(not $include_world_section)",
          "controls": [
      ]}}
  ]},
  "section_footer_panels": {
    "type": "panel",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "size": [ "100%", "100%c" ],
    "offset": [ 2, 0 ],
    "controls": [
      { "world_sections": {
          "type": "stack_panel",
          "ignored": "(not $include_world_section)",
          "controls": [
      ]}}
  ]},
  "play_or_create_stack_bedrock@world_section.launch_world_button_stack_def": {
    "ignored": "(not $is_pregame or $education_edition)",
    "controls": [
      { "create_or_play_button@aui-setting_screen.action_button": {
          "size": [ "fill", 35 ],
          "$button_text": "menu.play",
          "default_focus_precedence": 1,
          "$pressed_button_name": "play_world_button",
          "$focus_override_up": "FOCUS_OVERRIDE_STOP",
          "variables": [
            { "requires": "(not $is_world_edit)",
              "$button_text": "createWorldScreen.create",
              "$pressed_button_name": "create_world_button"
          }]
      }},
      { "play_on_realm_button@aui-setting_screen.action_button": {
          "size": [ "50%", 35 ],
          "ignored": "(not $is_pregame or $education_edition or $is_editor_mode_enabled)",
          "$button_text_max_size": [ "100%", 25 ],
          "$button_text": "menu.playOnRealms",
          "$pressed_button_name": "play_world_on_realm_button",
          "$focus_override_up": "FOCUS_OVERRIDE_STOP",
          "variables": [
            { "requires": "(not $is_world_edit)",
              "$button_text": "createWorldScreen.createOnRealms",
              "$pressed_button_name": "create_world_on_realm_button"
          }],
          "bindings": [
            { "binding_name": "#create_on_realm_visible",
              "binding_name_override": "#visible"
          }]
      }}
  ]},
  "toggle_button_control@auicore_configloader.core": {
    "type": "panel",
    "$glyph_offset|default": [ 5, 0 ],
    "$glyph_anchor_from|default": "left_middle",
    "$glyph_anchor_to|default": "left_middle",
    "$button_label_offset|default": [ 34, 0 ],
    "layer": 2,
    "controls": [
      { "glyph": {
          "type": "image",
          "anchor_from": "$glyph_anchor_from",
          "anchor_to": "$glyph_anchor_to",
          "offset": "$glyph_offset",
          "texture": "$glyph_texture",
          "size": "$glyph_size",
          "layer": 3,
          "color": "$glyph_color",
          "alpha": "$content_alpha",
          "$glyph_bindings|default": [],
          "bindings": "$glyph_bindings"
      }},
      { "glyph_color": {
          "type": "image",
          "anchor_from": "$glyph_anchor_from",
          "anchor_to": "$glyph_anchor_to",
          "offset": "$glyph_offset",
          "texture": "$glyph_color_texture",
          "size": "$glyph_size",
          "layer": 4,
          "color": "$setting_button_icon_color",
          "alpha": "$content_alpha",
          "$glyph_color_bindings|default": [],
          "bindings": "$glyph_color_bindings"
      }},
      { "tab_button_text": {
          "type": "label",
          "anchor_from": "left_middle",
          "anchor_to": "left_middle",
          "size": [ "100% - 38px", "default" ],
          "max_size": [ "100% - 38px", "100% - 4px" ],
          "offset": "$button_label_offset",
          "text": "$button_text",
          "layer": 3,
          "color": "$text_color",
          "alpha": "$content_alpha",
          "$button_text_binding_type|default": "none",
          "$button_text_grid_collection_name|default": "",
          "$button_text_binding_condition|default": "none",
          "bindings": [
            { "binding_type": "$button_text_binding_type",
              "binding_condition": "$button_text_binding_condition",
              "binding_collection_name": "$button_text_grid_collection_name",
              "binding_name": "$button_text",
              "binding_name_override": "$button_text"
          }]
      }}
  ]},
  "upload_world_stack_bedrock@world_section.launch_world_button_stack_def": {
    "ignored": "(not $is_pregame or $education_edition)",
    "controls": [
      { "upload_world_button@aui-setting_screen.action_button": {
          "size": [ "fill", 35 ],
          "$button_text": "createWorldScreen.upload",
          "$pressed_button_name": "upload_world_button"
    }}],
    "bindings": [
      { "binding_name": "#upload_world_visible",
        "binding_name_override": "#visible"
  }]},
  "addons_selector_panel": {
    "type": "stack_panel",
    "$tts_section_header": "options.category.addons",
    "visible": "(not $education_edition_china)",
    "controls": [
      { "pudding00": { "type": "panel", "size": [ 2, 2 ] }},
      { "header00@aui-common.header": { "$text": "Resources/Addons" }},
      { "pudding00": { "type": "panel", "size": [ 1, 1 ] }},
      { "level_texture_pack_button@aui-setting_screen.level_texture_pack_button": { "$toggle_group_forced_index": "$level_texture_pack_toggle_index" }},
      { "addon_button@aui-setting_screen.addon_button": { "$toggle_group_forced_index": "$addon_toggle_index" }}
  ]},
